<%= form_for @user , url: {action: "create"}, method: "post"  do |f| %>
    <div class='row'>
        <div class='col-md-4 col-sm-offset-4'>      
            <h2>Albums</h2>
        </div>

        <div class='col-md-4'>
            <%= link_to new_album_path do%>
            <i class="fa fa-plus fa-5x" aria-hidden="true"></i>
          <% end %> 
        </div>
    </div>
    

    <div class='row'>
        <% @albums.each do |item|%>
        
            <% image = item.album_images.first 
            next unless image.present?
        %>
       
        
            <div class='col-md-4'>
                <div class='col-md-12'>
                    <p><%= link_to item.album_name, new_album_path %></p>
                </div>
                    <div class='col-md-12'>
                        <div class='col-md-4'>
                            Add More
                        </div>
                        <div class='col-md-2'>
                            <%= link_to  album_path(id: item.id)  do%>
                        <i class="fa fa-plus fa-2x" aria-hidden="true"></i>
                      <% end %>
                        </div>
                        <div class='col-md-2 col-sm-offset-4'>
                            <%= link_to album_destroy_album_path(item.id), method: :delete ,  data: { confirm: 'Are you sure?' }  do%> 
                        <i class="fa fa-times fa-2x" aria-hidden="true"></i>
                      <% end %>     
                        </div>
                </div>
                <div class='col-md-12'>
                    <%= image_tag "/uploads/#{image.image_name}"    %>
                </div>
            </div>
        <% end %>
    </div>
    <% i=0 %>
<% end %>

<hr>
<hr>

<div class='row'>
    <div class='col-md-4 col-sm-offset-4'>      
        <h2>Friends-Albums</h2>
    </div>
</div>

<div class='row'>
    <% @friends.each do |friend| %>
        <% albums = Album.where(user_id: friend.id)%>
        <% albums.each do |item|%>
            <% img = item.album_images.first 
                next unless img.present?
          %>
            <div class='col-md-4'>
                
                <div class="col-md-12">
                    <p><%= link_to item.album_name, new_album_path %></p>
                </div>
                <div class='col-md-12'> 
                    <%= image_tag "/uploads/#{item.album_images.first.image_name}"  %>
                </div>
            </div>
        <% end %> 
    <% end %>   
</div>










